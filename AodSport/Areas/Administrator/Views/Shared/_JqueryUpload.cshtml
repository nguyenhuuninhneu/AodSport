<script>
    $(document).ready(function () {
        FormFileUpload.init("@changeName", "@type", null, null, null, "@auto", "@name", "@multiple", "@value", "@setname", "@valueName", "@setSize", "@valueSize", "@fileType", "@imageHoSo");
    });
</script>

<div class="row fileupload-buttonbar">
    <div class="col-lg-7">
        <!-- The fileinput-button span is used to style the file input field as button -->
        <span class="btn btn-success fileinput-button" id="btThem">
            <i class="fa fa-plus"></i>
            <span> Lựa chọn </span>
            <input type="hidden" id="@name" name="@name" value="@(value ?? "")">
            @*<input type="hidden" id="name_@name" name="name_@name" value="">*@
            <input type="hidden" id="@setname" data-id="@setname" name="@setname" value="@(valueName ?? "")" />
            <input type="hidden" id="@setSize" data-id="@setSize" name="@setSize" value="@(valueSize??"")" />
            <input type="file" data-id="@name" class="files_@name" @(multiple ? "multiple" : "") />

        </span>

    </div>
    <!-- The global progress state -->
    <div class="col-lg-5 fileupload-progress fade">
        <!-- The global progress bar -->
        <div class="progress progress-striped active" role="progressbar" aria-valuemin="0" aria-valuemax="100">
            <div class="progress-bar progress-bar-success" style="width:0%;"></div>
        </div>
        <!-- The extended global progress state -->
        <div class="progress-extended">&nbsp;</div>
    </div>
</div>

<div class="result-div"></div>

<table role="presentation" class="table table-striped table-uploadvx clearfix">
    <tbody class="result-table" id="table-edit-@name">
        @if (!string.IsNullOrEmpty(value)) // co valude -> edit
        {
            var arrFiles = value.Split(',');
            if (type == "image")
            {
                for (int i = 0; i < arrFiles.Length; i++)
                {
                    var linkImgEdit = arrFiles[i];
                    var nameImgEdit = linkImgEdit.Substring(linkImgEdit.LastIndexOf("/") + 1);
                    <tr class="image-download-image fade show imgPreviewJu">
                        <td colspan="3" class="tdimg">
                            <span class="preview">
                                <a href="@linkImgEdit" title="@nameImgEdit" download="@nameImgEdit" data-gallery><img src="@linkImgEdit" width="200"></a>
                            </span>
                            <a class="removeImgVz" data-url="/JqueryUpload/DeleteFile?url=@linkImgEdit" onclick="removeFileVz($(this))"><i class="fa fa-close" style="font-size: 25px;"></i></a>
                            <input type="hidden" name="linkFileImage" value="@linkImgEdit" />
                        </td>
                    </tr>
                }
            }
            else
            {
                var arrFileName = valueName.Split('|');
                var arrFileSize = valueSize.Split('|');
                var strFiles = "";
                string nameFile;
                string nameshow;
                string attchmentName;
                string attchmentSize;
                for (int i = 0; i < arrFiles.Length; i++)
                {
                    if (arrFiles[i] != "")
                    {
                        var linkFile = arrFiles[i];
                        nameFile = linkFile.Substring(linkFile.LastIndexOf("/") + 1);
                        attchmentName = arrFileName[i];
                        attchmentSize = arrFileSize[i];
                        nameshow = nameFile.Substring(15);
                        //if (attchmentName != "")
                        //{
                        //    nameshow = attchmentName;
                        //}
                        <tr class="template-download">
                                <td>
                                    <p class="name">
                                        <a href="@linkFile" title="@nameFile" download="@nameFile" >@nameshow</a>
                                           <input type="hidden" name="linkFile" value="@linkFile" /><span style="margin-left:5px;color:#337ab7">|</span>
                                        <a href="/Home/ViewFile?linkdown=@linkFile" target="_blank" style="margin-left: 5px; color: #337ab7"><i class="fa fa-search"></i> Xem </a>
                                    </p>
                                </td>
                                @if (changeName == true)
                                {
                                    <td width="160">
                                        <input class="form-control valid" type="text" id="abcd" name="abcd" value="@attchmentName" />
                                    </td>
                                }
                                <td width="80">
                                     <p class="size" id="size" name = "size">@attchmentSize</p>
                                       <input type="hidden" name="sizeFile" value="@attchmentSize" />
                                     <div class="progress progress-striped active" role="progressbar" aria-valuemin="0" aria-valuemax="100" aria-valuenow="0">
                                           <div class="progress-bar progress-bar-success" style="width:0%;"></div>
                                     </div>
                               </td>
                                 <td width="80">
                                  <button class="btn btn-danger delete" data-type="GET" data-url="/JqueryUpload/DeleteFile?url=@nameFile" data-delete="/Content/Upload/@nameFile">Xóa</button>
                                 </td>
                            </tr>
                    }
                }
            }
        }
    </tbody>
</table>

<script id="document-upload" type="text/x-tmpl">

    {% for (var i=0, file; file=o.files[i]; i++) { %}
    <tr class="template-upload fade">
        <td>
            <p class="name">{%=file.name%}</p>
            <strong class="error text-danger label label-danger"></strong>
            <input type="hidden" name="linkFile" value="" />
        </td>
        @if (setname != null && changeName == true)
        {
            <td width="160"><input class="form-control valid" style="width:100%" type="text" id="abcd" name="abcd" value="" /></td>
        }
        <td>
            <p class="size">Đang tải...</p>
            <div class="progress progress-striped active" role="progressbar" aria-valuemin="0" aria-valuemax="100" aria-valuenow="0">
                <div class="progress-bar progress-bar-success" style="width:0%;"></div>
            </div>
            <input type="hidden" name="sizeFile" value="" />
        </td>
        <td width="160">
            {% if (!i && !o.options.autoUpload) { %}
            <button class="btn btn-primary start" disabled>
                <i class="glyphicon glyphicon-upload"></i>
                <span>Tải lên</span>
            </button> {% } %} {% if (!i) { %}
            <button class="btn btn-warning cancel">
                <i class="glyphicon glyphicon-cancel"></i>
                <span>Hủy</span>
            </button> {% } %}
        </td>
    </tr> {% } %}
</script>
<!-- The template to display files available for download -->
<script id="document-download" type="text/x-tmpl">
    {% for (var i=0, file; file=o.files[i]; i++) { %}
    <tr class="template-download fade">
        <td>
            <p class="name">
                <a href="{%=file.url%}" title="{%=file.name%}" download="{%=file.name%}" {%=file.thumbnailUrl?'data-gallery':''%}>
                    {%=file.name%}
                </a>
                <span style="margin-left:5px;color:#337ab7">|</span>
                <a href="/Home/ViewFile?linkdown={%=file.url%}" target="_blank" style="color:#337ab7">
                    <i style="padding-left:5px;" class="fa fa-search"></i> Xem
                </a>
                <input type="hidden" name="linkFile" value="{%=file.url%}" />
            </p>
            {% if (file.error) { %}
            <div><span class="error">Error</span> {%=file.error%}</div>
            {% } %}
        </td>
        @if (changeName == true)
        {
            <td width="160">
                <input class="form-control valid" style="width:100%" type="text" id="abcd" name="abcd" value="{%=file.nameFile%}" />
            </td>
        }

        <td width="80">
            <span class="size" id="AttchmentSize">{%=o.formatFileSize(file.size)%}</span>
            <input type="hidden" name="sizeFile" value="{%=o.formatFileSize(file.size)%}" />
        </td>
        <td width="80">
            <button class="btn btn-danger delete" data-type="{%=file.deleteType%}" data-url="{%=file.deleteUrl%}" data-delete="{%=file.url%}" {% if (file.deletewithcredentials) { %} data-xhr-fields='{"withCredentials":true}' {% } %}>Xóa</button>

        </td>
    </tr>
    {% } %}
</script>

<script id="template-upload" type="text/x-tmpl">
    {% for (var i=0, file; file=o.files[i]; i++) { %}
    <tr class="template-upload fade in">
        <td>
            <span class="preview"></span>
        </td>
        <td>
            <p class="name">{%=file.name%}</p>
            <strong class="error text-danger"></strong>
        </td>
        <td width="80">
            <p class="size">{%=o.formatFileSize(file.size)%}</p>
            <div class="progress progress-striped active" role="progressbar" aria-valuemin="0" aria-valuemax="100" aria-valuenow="0"><div class="progress-bar progress-bar-success" style="width:0%;"></div></div>
        </td>
        <td width="160">
            {% if (!i && !o.options.autoUpload) { %}
            <button class="btn btn-primary start" disabled>
                <i class="glyphicon glyphicon-upload"></i>
                <span>Tải lên</span>
            </button> {% } %} {% if (!i) { %}
            <button class="btn btn-warning cancel">
                <i class="glyphicon glyphicon-cancel"></i>
                <span>Hủy</span>
            </button> {% } %}
        </td>
    </tr>
    {% } %}
</script>
<!-- The template to display files available for download -->
<script id="template-download" type="text/x-tmpl">
    {% for (var i=0, file; file=o.files[i]; i++) { %}
    <tr class="template-download fade">
        <td>
            <span class="preview">
                {% if (file.thumbnailUrl) { %}
                <a href="{%=file.url%}" title="{%=file.name%}" download="{%=file.name%}" data-gallery><img src="{%=file.thumbnailUrl%}"></a>
                {% } %}
            </span>
        </td>
        <td width="160">
            <p class="name">
                <a href="{%=file.url%}" title="{%=file.name%}" download="{%=file.name%}" {%=file.thumbnailUrl?'data-gallery':''%}>{%=file.name%}</a>
            </p>
            {% if (file.error) { %}
            <div><span class="error">Error</span> {%=file.error%}</div>
            {% } %}
        </td>
        <td width="80">
            <span class="size">{%=o.formatFileSize(file.size)%}</span>
        </td>
        <td width="80">
            <button class="delete" data-type="{%=file.deleteType%}" data-url="{%=file.deleteUrl%}" {% if (file.deletewithcredentials) { %} data-xhr-fields='{"withCredentials":true}' {% } %}>Xóa</button>
        </td>
    </tr>
    {% } %}
</script>


